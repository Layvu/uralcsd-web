    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "actors",
        "management",
        "directors"
      ]
    },
    "mainPhoto": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "biography": {
      "type": "richtext"
    },
    "position": {
      "type": "string"
    },
    "performanceCasts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::performance-cast.performance-cast",
      "mappedBy": "actor"
    },
    "aPerformances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::performance.performance",
      "mappedBy": "director"
    }
